
@import 'style/weui.wxss';
page {
    background-color: #ffffff;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.nav{
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  background-color: #ffffff;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 43px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  font-size:34rpx;
  letter-spacing:2px;
  background-color: #ffffff;
}
.nav .back{
  width: 22px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
  background-color: #ffffff;
}
.bg-white{
  background-color: #ffffff;
}
.bg-gray{
  background-color: #ffffff;
}
.overflow{
  overflow: auto;
}
.hidden{
  overflow: hidden;
}